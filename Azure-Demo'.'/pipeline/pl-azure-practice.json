{
	"name": "pl-azure-practice",
	"properties": {
		"description": "first pipline",
		"activities": [
			{
				"name": "adf-copy-demo",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_adf_demo",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "adf-practice"
		},
		"annotations": []
	}
}